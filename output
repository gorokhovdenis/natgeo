<script type="text/javascript">
		$(function () {
			layout.widths = {  feedTablet:980, feedPadding:40, itemMargin:40, stickyNavigation:60, viewport:1024, scrollbar:17, desktop:1660, tablet:1024, retina:640, mobile:320, feedDesktop:1660, feedLaptop:1320, feedRetina:640, feedMobile:300, itemLargeWidth:640, itemSmallWidth:300, itemSmallHeight:169, leaderboard:728, leaderboardPadding:10, exploreTablet:900, explorePadding:40, logo:134, hamburger:1240, pillarCollapse:1374, pillarStack:1108, navigation:60 };
			layout.master();
			$('.Skin a').click(function (event) {
				var href = $(this).attr('href');
				event.preventDefault();
				secondTracker._trackEvent('Skin', 'Click', href);
				setTimeout(function () {
					window.location.href = href;
				}, 200);
			});
			$('.JamesCameronMrec').click(function (event) {
				var href = $(this).attr('href');
				event.preventDefault();
				secondTracker._trackEvent('Mrec', 'Click', href);
				setTimeout(function () {
					window.location.href = href;
				}, 200);
			});

			// Outbound link tracking
			$('a').filter(function () {
				var href = $(this).attr('href');
				return href !== undefined && href !== null && href.length && href.indexOf('://') !== -1 && href.indexOf('nationalgeographic.com.au') === -1;
			}).click(function (event) {
				var href = $(this).attr('href');
				event.preventDefault();
				secondTracker._trackEvent('Outbound Link', 'Click', href);
				setTimeout(function () {
					window.location.href = href;
				}, 100);
			});

		});
	</script>
<!-- Plastics Pledge -->
<script type="text/javascript">
		$(function () {
			if ($('.PlasticsPledge').length) {
				plasticsPrevScrollTop = 0;
				$(window).scroll(function () {
					if ($(window).scrollTop() + $(window).height() < $('.Newsletter').offset().top) {
						$('.PlasticsPledge').addClass('Visible');
					} else {
						$('.PlasticsPledge').removeClass('Visible');
					}
					if (Math.abs($(window).scrollTop() - plasticsPrevScrollTop) > 100) {
						plasticsPrevScrollTop = $(window).scrollTop();
					}
				});
			}
		});
	</script>
